{
  "version": 2,
  "builds": [
    { "src": "public/index.html", "use": "@vercel/static" },
    { "src": "spotify/index.html", "use": "@vercel/static" },
    { "src": "netflix/index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/spotify", "dest": "/spotify/index.html" },
    { "src": "/netflix", "dest": "/netflix/index.html" }
  ]
}